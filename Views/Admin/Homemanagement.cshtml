                        <a href="index.html" class="footer-logo">Y<span>oo</span>ga</a>
                        <h3>11821 Mecca St. Amman, Jordan</h3>
                        <div class="footer-menu">
                            <p>+692 7883 18984</p>
                            <p>YooGACenter@gmail.com</p>
                        </div>
                        <div class="footer-social">
                            <a href=""><i class="fab fa-twitter"></i></a>
                            <a href=""><i class="fab fa-facebook-f"></i></a>
                            <a href=""><i class="fab fa-youtube"></i></a>
                            <a href=""><i class="fab fa-instagram"></i></a>
                            <a href=""><i class="fab fa-linkedin-in"></i></a>
                        </div>
                    </div>
                </div>
                <div class="container copyright">
                    <div class="row">
                        <div class="col-md-6">
                            <p>&copy; <a href="#">YooGA</a>, All Right Reserved.</p>
                        </div>
                        
                    </div>
                </div>
            </div>
        </div>
        <!-- Footer End -->

        <a href="#" class="back-to-top"><i class="fa fa-chevron-up"></i></a>

        <!-- JavaScript Libraries -->
        <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"></script>
        <script src="~/HomeAssets/lib/easing/easing.min.js"></script>
        <script src="~/HomeAssets/lib/wow/wow.min.js"></script>
        <script src="~/HomeAssets/lib/owlcarousel/owl.carousel.min.js"></script>
        <script src="~/HomeAssets/lib/isotope/isotope.pkgd.min.js"></script>
        <script src="~/HomeAssets/lib/lightbox/js/lightbox.min.js"></script>

        <!-- Contact Javascript File -->
        <script src="~/HomeAssets/mail/jqBootstrapValidation.min.js"></script>
        <script src="~/HomeAssets/mail/contact.js"></script>

        <!-- Template Javascript -->
        <script src="~/HomeAssets/js/main.js"></script>
